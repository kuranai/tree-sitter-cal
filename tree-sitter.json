{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "cal",
      "camelcase": "Cal",
      "scope": "source.c-al",
      "path": ".",
      "file-types": ["txt"],
      "highlights": "queries/highlights.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for Dynamics NAV C/AL",
    "authors": [
      {
        "name": "Codex"
      }
    ],
    "links": {
      "repository": "https://example.com/tree-sitter-cal"
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "node": true,
    "python": false,
    "rust": false,
    "swift": false
  }
}
